   "id": "e41c2df2-264d-4742-9a76-e7d4acc7561f",
   "metadata": {},
   "outputs": [],
   "source": [
    "#The lambda x: x.day part is a small anonymous function that extracts the day of the month (from 1 to 31) from the datetime object x.\n",
    "#The lambda x: x.weekday() function extracts the day of the week as a numerical value (0 for Monday, 6 for Sunday) from the datetime object x.\n",
    "#The lambda x: x.hour function extracts the hour (from 0 to 23) from the datetime object x.\n",
    "\n",
    "data['Day'] = data['Date/Time'].apply(lambda x: x.day)\n",
    "data['Weekday'] = data['Date/Time'].apply(lambda x: x.weekday())\n",
    "data['Hour'] = data['Date/Time'].apply(lambda x: x.hour)"
   ]
  },
